<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="FlashLightApp2018.MorseCode">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Morse Code Page!"
                VerticalOptions="CenterAndExpand" 
                HorizontalOptions="CenterAndExpand" />

            <Button Text="SOS" x:Name="SOSBtn" Clicked="SOSBtn_Clicked"/>
            <Button Text="Hello" x:Name="HelloBtn" Clicked="HelloBtn_Clicked"/>
            <Button Text="Warning" x:Name="WarningBtn" Clicked="WarningBtn_Clicked"/>
            <Button Text="WOW" x:Name="WOWBtn" Clicked="WOWBtn_Clicked"/>
            <Button Text="LOL" x:Name="LOLBtn" Clicked="LOLBtn_Clicked"/>
            <Button Text="911" x:Name="EmergancyBtn" Clicked="EmergancyBtn_Clicked"/>
            <Button Text="Bye" x:Name="ByeBtn" Clicked="ByeBtn_Clicked"/>
            <Button Text="GMIT" x:Name="GMITBtn" Clicked="GMITBtn_Clicked"/>

        </StackLayout>
    </ContentPage.Content>
</ContentPage>